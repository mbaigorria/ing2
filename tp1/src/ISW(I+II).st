Simulacion registroDeActividades.	unEscenarioDeSimulacion infraestructuraDisponible redDeDistribucionConectada incorporarTanqueDeAgua: unTanqueDeAgua	! !!Constructora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 05:03'!realizarConstruccionesDe: unEscenarioDeSimulacion	| obrasAConstruir infraestructura registro  |	registro := unEscenarioDeSimulacion registroDeActividades.	infraestructura := unEscenarioDeSimulacion infraestructuraDisponible.	obrasAConstruir := unEscenarioDeSimulacion proyectoEnCurso planDeHoy planDeConstruccion obrasAConstruir.	obrasAConstruir do: [ :obra | 		obra asignarRegistroDeActividades: registro.		infraestructura prepararNuevaObraDeConstruccion: obra ].	infraestructura continuarConstruccionesCon: self en: unEscenarioDeSimulacion. 		! !!Constructora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!inaugurarPlantaSeparadora: unaPlantaDeSeparacion en: unEscenarioDeSimulacion	unaPlantaDeSeparacion asignarRegistroDeActividades: unEscenarioDeSimulacion registroDeActividades.	unEscenarioDeSimulacion infraestructuraDisponible redDeDistribucionConectada incorporarPlantaSeparadora: unaPlantaDeSeparacion	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Constructora class	instanceVariableNames: ''!!Constructora class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 00:12'!nuevaConstructora	^(self new) inicializar! !Object subclass: #DepositoDeEquipamiento	instanceVariableNames: 'rigsDepositados calendario carpetaDeDocumentacionDeAlquileres'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 18:05'!inicializar	calendario := Calendario iniciadoEn: (Date today).	rigsDepositados := OrderedCollection new.	carpetaDeDocumentacionDeAlquileres := OrderedCollection new.	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/17/2017 19:57'!depositarRIG: unRIG 	rigsDepositados add: unRIG.	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 04:23'!documentarAlquiler: unAlquiler 	carpetaDeDocumentacionDeAlquileres add: (DocumentoDeAlquiler para: unAlquiler enFecha: (calendario fechaDeHoy)).	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:00'!sincronizarAgendaConCalendario: unCalendario	calendario := unCalendario! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 04:53'!cancelarAlquilerDeDcumentacion: unDocumentoDeAlquiler	| diasAlquilado diasMinimos |		diasAlquilado :=  (calendario fechaDeHoy) subtractDate: (unDocumentoDeAlquiler fechaEnQueSeAlquilo).	diasMinimos := unDocumentoDeAlquiler alquilerDocumentado diasMinimos.	(((diasMinimos - diasAlquilado) <= 0) & (rigsDepositados includes: (unDocumentoDeAlquiler alquilerDocumentado rigEnAlquiler)))	ifTrue: [ carpetaDeDocumentacionDeAlquileres remove: unDocumentoDeAlquiler. 		^true ]	ifFalse: [ ^false ]	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/17/2017 20:00'!inventarioDeRIGS	^rigsDepositados 	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/17/2017 20:02'!retirarRIG: unRIG 	rigsDepositados remove: unRIG.	! !!DepositoDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:00'!documentosDeAlquileresRegistrados	^carpetaDeDocumentacionDeAlquileres	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!DepositoDeEquipamiento class	instanceVariableNames: ''!!DepositoDeEquipamiento class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 19:09'!inaugurarConCalendario: unCalendario	^(self new) inicializar; sincronizarAgendaConCalendario: unCalendario! !Object subclass: #DirectorioDeIngenieria	instanceVariableNames: 'politicaDeDistribucion politicaDeExcavacion politicaDeConstruccion pol