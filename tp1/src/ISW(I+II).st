rioDeIngenieria seguirPoliticasDeExcavacion: (OrderedCollection withAll: (unaColeccionDeNumeros collect: [ :num | opciones at: num ifAbsent: [ opciones at: 1 ]])).	Transcript clear.	! !!InterfazDeSimulador methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 06:04'!configurarValorDePresionCritica: unValor	simulador configurarValorDePresionCritica: unValor! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!InterfazDeSimulador class	instanceVariableNames: ''!!InterfazDeSimulador class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/6/2017 04:34'!para: unSimulador	^(self new) representarSimulador: unSimulador! !Catalogo subclass: #InventarioDeCatalogo	instanceVariableNames: 'publicaciones'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!InventarioDeCatalogo methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:15'!inicializar	publicaciones := OrderedCollection new.! !!InventarioDeCatalogo methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:15'!publicaciones	^publicaciones! !!InventarioDeCatalogo methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:16'!confeccionarCopia	| repertorio |	repertorio := (self class new) inicializar.	publicaciones do: [ :pub | 		repertorio publicar: pub.		].	^repertorio! !!InventarioDeCatalogo methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/16/2017 18:04'!publicar: unPublicable	^self subclassResponsibility! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!InventarioDeCatalogo class	instanceVariableNames: ''!!InventarioDeCatalogo class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 17:20'!diseñoSinPublicaciones	^(self new) inicializar! !InventarioDeCatalogo subclass: #InventarioDeAlquileres	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!InventarioDeAlquileres methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:16'!publicar: unaObraConstruccion	publicaciones add: unaObraConstruccion! !InventarioDeCatalogo subclass: #InventarioDeObrasDeConstruccion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!InventarioDeObrasDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:20'!publicar: unaObraConstruccion 	publicaciones add: unaObraConstruccion! !Object subclass: #MapaDeParcelas	instanceVariableNames: 'parcelas excavaciones'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!MapaDeParcelas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:25'!inicializarConParcelas: unaColeccionDeParcelas	parcelas := unaColeccionDeParcelas.	excavaciones := OrderedCollection new.! !!MapaDeParcelas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 16:17'!cantidadDeExcavacionesEnCurso	^excavaciones size! !!MapaDeParcelas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:30'!parcelasEnMapa	^parcelas! !!MapaDeParcelas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:29'!proseguirConExcavaciones	| terminadas |	excavaciones do: [ :excavacion  | excavacion proseguirConExcavacion ].	terminadas := excavaciones select: [ :excavacion | (excavacion parcelaEnExcavacion distanciaAlReservorio) <= (excavacion distanciaExcavada) ].	excavaciones removeAll: terminadas.	^terminadas! !!MapaDeParcelas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:35'!indicarNuevaExcavacion: unaExcavacion	excavaciones detect: [ :exc | (exc parcelaEnExcavacion = unaExcavacion parcelaEnExcavacion) ] ifNone: [ 	excavaciones add: unaExcavacion ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!MapaDeParcelas class	instanceVariableNames: ''!!MapaDeParcelas class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 17:11'!cartografiarEnBaseA: unaColeccionDeParcelas	^(self new) inicializarConParcelas: unaColeccionDeParcelas! !ConfiguracionDeValvula subclass: #ModoDeExtraccion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ModoDeExtraccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:06'!distribuir: unaValvulaDePozo	| capacidadDeRed producto volExtraido volAceptado red latitud longitud ubicacion datos capacidadDeRedDsp capacidadDeSepaAntes volProcesado |	producto := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos tipoDeProductoAlbergado.	red := unaValvulaDePozo redDeConexion redDeDistribucionConectada.	capacidadDeRed := red capacidadParaProducto: producto.	capacidadDeSepaAntes := red capacidadDeSeparacion.	volAceptado := volumenConfigurado min: capacidadDeRed.	volExtraido := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos extraer: volAceptado.	volProcesado := red procesarCaudalDe: volExtraido deProducto: producto.		ubicacion := unaValvulaDePozo parcelaDelPozo ubicacion.	latitud := ubicacion x.	longitud := ubicacion y.	datos := OrderedCollection new.	datos add: volExtraido; add: latitud; add: longitud.	unaValvulaDePozo registrar: ('Se extrajeron {1}m3 del pozo en ubicacion ({2},{3}).' format: datos).		^volProcesado	! !!ModoDeExtraccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 20:22'!actualizarPresion: unaValvulaDePozo	| volInicial volActual cantDePozos beta|	volInicial := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos volumenInicialmenteAlmacenado.	volActual := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos volumenActualmenteAlmacenado.	cantDePozos := unaValvulaDePozo redDeConexion cantidadDePozos.	beta := (0.1*(volActual/volInicial)/( (cantDePozos raisedTo: 2) nthRoot: 3 ) ).	^(unaValvulaDePozo medirPresionDeBocaDePozo)*((Float e) raisedTo: (beta negated)).! !ConfiguracionDeValvula subclass: #ModoDeReinyeccion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ModoDeReinyeccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 20:23'!actualizarPresion: unaValvulaDePozo	| volGlobalExtraido volGlobalReinyectado volInicial |	volGlobalExtraido := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos volumenTotalExtraido.	volGlobalReinyectado := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos volumenTotalReinyectado.	volInicial := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos volumenInicialmenteAlmacenado.	^(unaValvulaDePozo parcelaDelPozo presionInicial)*(volInicial - volGlobalExtraido + volGlobalReinyectado)/volInicial.! !!ModoDeReinyeccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/29/2017 05:59'!distribuir: unaValvulaDePozo	^self subclassResponsibility! !ModoDeReinyeccion subclass: #ModoDeReinyeccionDeAgua	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ModoDeReinyeccionDeAgua methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 21:50'!distribuir: unaValvulaDePozo	| volReinyectado latitud longitud ubicacion datos volumenDesdeRed |	volumenDesdeRed := unaValvulaDePozo redDeConexion redDeDistribucionConectada extraerVolumenDeAguaAlmacenada: volumenConfigurado.	volReinyectado := unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos reinyectar: volumenDesdeRed.	 	ubicacion := unaValvulaDePozo parcelaDelPozo ubicacion.	latitud := ubicacion x.	longitud := ubicacion y.	datos := OrderedCollection new.	datos add: volReinyectado; add: latitud; add: longitud; add: (volumenDesdeRed); add: (unaValvulaDePozo redDeConexion reservorioAlQueDesembocanLosPozos tipoDeProductoAlbergado proporcionDePetroleo); add: (unaValvulaDePozo medirPresionDeBocaDePozo).	unaValvulaDePozo registrar: ('Se reinyecto {1}m3 de agua con el pozo en ubicacion ({2},{3}), volReinyectado: {4}, proporcionDePetroleo: {5}, presion: {6}' format: datos).		^volReinyectado! !ModoDeReinyeccion subclass: #ModoDeReinyeccionDeGas	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ModoDeReinyeccionDeGas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 21:50'!distribuir: unaValvulaDePozo	| volReinyectado latitud longitud ubicacion datos volumenDesdeRed |	volumenDesdeRed := unaValvulaDePozo redDeConexion redDeDistribucionConectada extraerVolumenDeGasAlmacenado: volumenConfigurado.	volReinyectado := unaValvulaDePozo reservorioDelYacimiento reinyectar: volumenDesdeRed.	 	ubicacion := unaValvulaDePozo parcelaDelPozo ubicacion.	latitud := ubicacion x.	longitud := ubicacion y.	datos := OrderedCollection new.	datos add: volReinyectado; add: latitud; add: longitud.	unaValvulaDePozo registrar: ('Se reinyecto {1}m3 de gas con el pozo en ubicacion ({2},{3}).' format: datos).		^volReinyectado! !Object subclass: #MotivoDeDistribucion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!MotivoDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 21:38'!distribuirEn: unEscenarioDeSimulacion mediante: unDistribuidorDeProducto	^self subclassResponsibility ! !MotivoDeDistribucion subclass: #Extraccion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Extraccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 21:38'!distribuirEn: unEscenarioDeSimulacion mediante: unDistribuidorDeProducto	^unDistribuidorDeProducto realizarExtracionEn: unEscenarioDeSimulacion.! !Object subclass: #Parcela	instanceVariableNames: 'ubicacion presionInicial tipoDeTerreno distanciaAlReservorio estado'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Parcela methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 16:59'!perforarCon: unRIG en: unEscenarioDeSimulacion mediante: unExcavador	estado := estado perforar: self en: unEscenarioDeSimulacion con: unRIG mediante: unExcavador! !!Parcela methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:51'!excavarCon: unRIG habiendoExcavado: unaDistancia	| excavado |	excavado := (unRIG poderDeExcavacion)/(tipoDeTerreno densidad) + unaDistancia.	excavado := distanciaAlReservorio min: excavado.	^(excavado - unaDistancia)	! !!Parcela methodsFor: 'initialization' stamp: 'Cherubini 7/19/2017 18:15'!inicializarEn: unaUbicacion conTipoDeTerreno: unTipoDeTerreno aDistanciaDelReservorio: unaDistancia yPresion: unaPresion 	ubicacion := unaUbicacion.	tipoDeTerreno := unTipoDeTerreno.	distanciaAlReservorio := unaDistancia.	presionInicial := unaPresion.	estado := SinPerforar new.! !!Parcela methodsFor: 'accessing' stamp: 'Cherubini 5/11/2017 00:59'!tipoDeTerreno	^ tipoDeTerreno! !!Parcela methodsFor: 'accessing' stamp: 'Cherubini 5/10/2017 22:44'!distanciaAlReservorio	^distanciaAlReservorio! !!Parcela methodsFor: 'accessing' stamp: 'Cherubini 5/10/2017 00:04'!ubicacion	^ ubicacion! !!Parcela methodsFor: 'accessing' stamp: 'Cherubini 7/15/2017 16:46'!presionInicial	^ presionInicial! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Parcela class	instanceVariableNames: ''!!Parcela class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 19:22'!en: unaUbicacion conTerreno: unTipoDeTerreno aDistanciaDelReservorio: unaDistancia yPresion: unaPresion 	^(self new) inicializarEn: unaUbicacion conTipoDeTerreno: unTipoDeTerreno aDistanciaDelReservorio: unaDistancia yPresion: unaPresion ! !EstadoDeParcela subclass: #Perforada	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Perforada methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 18:15'!perforar: unaParcela en: unEscenarioDeSimulacion con: unRIG mediante: unExcavador 	^Perforada new! !Object subclass: #PlanDeConstruccion	instanceVariableNames: 'obrasDeConstruccion'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlanDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 05:05'!documentarObrasDeConstruccion: unaColeccionDeObrasDeConstrucion	obrasDeConstruccion := unaColeccionDeObrasDeConstrucion! !!PlanDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 05:05'!inicializar	obrasDeConstruccion := OrderedCollection new.! !!PlanDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 05:06'!obrasAConstruir	^obrasDeConstruccion! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlanDeConstruccion class	instanceVariableNames: ''!!PlanDeConstruccion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:12'!diseñoInicial	^(self new) inicializar.! !Object subclass: #PlanDeDistribucion	instanceVariableNames: 'valvulas volumenesParaValvulas volumenDeGasAVender motivo ordenesDeValvulas'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 07:07'!volumenPara: unaValvula	^volumenesParaValvulas at: unaValvula.! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 18:54'!asignarVolumenes: unaColeccionDeVolumenes	| colVolumenes |	colVolumenes := unaColeccionDeVolumenes.	1 to: ((valvulas size) min: (colVolumenes size)) do: [ :index | volumenesParaValvulas at: (valvulas at: index) put: ((colVolumenes at: index) max: 0) ].! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 04:58'!inicializarConValvulas: unaColeccionDeValvulas	valvulas := unaColeccionDeValvulas.	volumenesParaValvulas := Dictionary new.	valvulas do: [ :val | volumenesParaValvulas at: val put: 0].	ordenesDeValvulas := OrderedCollection new.	motivo := Extraccion new.	volumenDeGasAVender := 0.! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/31/2017 01:17'!valvulasAHabilitar	^valvulas! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 21:38'!distribuirEn: unEscenarioDeSimulacion mediante: unDistribuidorDeProducto	motivo distribuirEn: unEscenarioDeSimulacion mediante: unDistribuidorDeProducto! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 21:36'!documentarMotivoDeDistribucion: unMotivoDeDistribucion	motivo := unMotivoDeDistribucion! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 18:53'!documentarGasAVender: unVolumen	volumenDeGasAVender := unVolumen max: 0! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 04:07'!documentarValvulas: unaColeccionPriorizadaDeValvulas	| newBlock |	valvulas := valvulas intersection: unaColeccionPriorizadaDeValvulas.	newBlock := unaColeccionPriorizadaDeValvulas sortBlock.	ordenesDeValvulas addFirst: newBlock.	volumenesParaValvulas removeAll.	valvulas do: [ :val | volumenesParaValvulas at: val put: 0].	ordenesDeValvulas do: [ :ord | valvulas sort: ord ].! !!PlanDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/4/2017 19:34'!volumenDeGasAVender	^volumenDeGasAVender! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlanDeDistribucion class	instanceVariableNames: ''!!PlanDeDistribucion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 03:38'!diseñoInicial	^(self new) inicializarConValvulas: OrderedCollection new.! !Object subclass: #PlanDeEquipamiento	instanceVariableNames: 'adquisiciones prescindibles'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlanDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:09'!documentarAlquileresParaAdquirir: unaColeccionDeAlquileres	adquisiciones := unaColeccionDeAlquileres.! !!PlanDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:07'!alquileresDeEquiposParaAdquirir	^adquisiciones! !!PlanDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:06'!documentarAlquileresPrescindibles: unaColeccionDeDocumentosDeAlquiler	prescindibles := unaColeccionDeDocumentosDeAlquiler.! !!PlanDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:09'!documentosDeAlquileresParaPrescindir	^prescindibles! !!PlanDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:06'!inicializar	adquisiciones := OrderedCollection new.	prescindibles := OrderedCollection new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlanDeEquipamiento class	instanceVariableNames: ''!!PlanDeEquipamiento class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:12'!diseñoInicial	^(self new) inicializar.! !Object subclass: #PlanDeExcavacion	instanceVariableNames: 'parcelas excavaciones ordenesDeRIGS ordenesDeParcelas'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 16:23'!rigAsignadoA: unaParcela	^excavaciones at: unaParcela.! !!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 05:06'!inicializarConParcelas: unaColeccionDeParcelas	excavaciones := Dictionary new.	parcelas := unaColeccionDeParcelas.	ordenesDeParcelas := OrderedCollection new.! !!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 05:07'!parcelasDocumentadas	^parcelas asOrderedCollection! !!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 17:43'!asignarRIGS: unaColeccionDeRIGS	| colRIGS|	colRIGS := unaColeccionDeRIGS.	1 to: ((parcelas size) min: (colRIGS size)) do: [ :index | excavaciones at: (parcelas at: index) put: (colRIGS at: index) ].! !!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:06'!documentarParcelas: unaColeccionPriorizadaDeParcelas	| newBlock |	parcelas := parcelas intersection: unaColeccionPriorizadaDeParcelas.	newBlock := unaColeccionPriorizadaDeParcelas sortBlock.	ordenesDeParcelas addFirst: newBlock.	ordenesDeParcelas do: [ :ord | parcelas sort: ord ].! !!PlanDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 05:07'!parcelasAPerforar	^excavaciones keys! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlanDeExcavacion class	instanceVariableNames: ''!!PlanDeExcavacion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:12'!diseñoInicial	^(self new) inicializarConParcelas: OrderedCollection new.! !Object subclass: #PlanDiarioDeProyecto	instanceVariableNames: 'planDeDistribucion planDeExcavacion planDeConstruccion planDeEquipamiento'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlanDiarioDeProyecto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 00:11'!planDeExcavacion	^planDeExcavacion! !!PlanDiarioDeProyecto methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:13'!inicializar	planDeDistribucion := PlanDeDistribucion diseñoInicial.	planDeEquipamiento := PlanDeEquipamiento diseñoInicial.	planDeConstruccion:= PlanDeConstruccion diseñoInicial.	planDeExcavacion := PlanDeExcavacion diseñoInicial.! !!PlanDiarioDeProyecto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 00:15'!planDeConstruccion	^planDeConstruccion! !!PlanDiarioDeProyecto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/29/2017 07:09'!planDeEquipamiento	^planDeEquipamiento! !!PlanDiarioDeProyecto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 00:21'!planDeDistribucion	^planDeDistribucion! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlanDiarioDeProyecto class	instanceVariableNames: ''!!PlanDiarioDeProyecto class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 22:11'!diseñoInicial	^(self new) inicializar! !Object 