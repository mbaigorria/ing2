ar	especificacionDeConstruccion := EspecificacionDePolitica especificar: [ :p |		""		| publicaciones obras cantidadDeEjemplar |		obras := OrderedCollection new.				(( (p fechaDeInicioDelProyecto = (p fechaDeHoy) ) | (p valvulasDePozoYaInstaladas isNotEmpty)) & (0.8*(p capacidadActualDeAlmacenamientoDeAgua) <= (p capacidadDeRedDeDistribucion)) & (p obrasActualmenteEnConstruccion isEmpty)) ifTrue: [		publicaciones := (p catalogoDeRecursosParaProyecto seccion: 'ObrasDeConstruccion') publicaciones.		cantidadDeEjemplar := (p valvulasDePozoYaInstaladas size) max: 1.		publicaciones do: [ :publicacion | obras add: publicacion withOccurrences: cantidadDeEjemplar ].		"repertorio := catalogoDeConstruccion subseccion: #('Almacenaje' 'TanquesDeAgua').		repertorio actualizarTodosLosStockA: (p mapaDeParcelas parcelasEnMapa size).		repertorio := catalogoDeConstruccion subseccion: #('Almacenaje' 'TanquesDeGas').		repertorio actualizarTodosLosStockA: (p mapaDeParcelas parcelasEnMapa size).		repertorio := catalogoDeConstruccion subseccion: #('Separacion' 'PlantasSeparadoras').		repertorio actualizarTodosLosStockA: (p mapaDeParcelas parcelasEnMapa size)."		].	obras.	].		descripcion := ''! !PoliticaDeConstruccion subclass: #PoliticaDeConstruccionVariada	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeConstruccionVariada methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 05:31'!inicializar	especificacionDeConstruccion := EspecificacionDePolitica especificar: [ :p |		"El primer dia del proyecto construye n tanque y plantas separadoras de cada modelo publicado, donde n es la cantidad de parcelas"		| publicaciones obras cantidadDeEjemplar |		obras := OrderedCollection new.				(p fechaDeInicioDelProyecto = p fechaDeHoy) ifTrue: [			publicaciones := (p catalogoDeRecursosParaProyecto seccion: 'ObrasDeConstruccion') publicaciones.				cantidadDeEjemplar := p mapaDeParcelas parcelasEnMapa size.		publicaciones do: [ :publicacion | obras add: publicacion withOccurrences: cantidadDeEjemplar ].		].		obras.		].		descripcion := 'El primer dia del proyecto construye n tanque y plantas separadoras de cada modelo publicado, donde n es la cantidad de parcelas'! !PoliticaDeProyecto subclass: #PoliticaDeDistribucion	instanceVariableNames: 'especificacionDeMotivo especificacionDeValvulas especificacionDeVolumenes especificacionDeVentaDeGas'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 21:36'!aplicarEn: unProyectoPetrolifero	| proyecto plan |	proyecto := unProyectoPetrolifero.	plan := proyecto planDeHoy planDeDistribucion.	plan documentarMotivoDeDistribucion: (especificacionDeMotivo aplicarEn: proyecto).	plan documentarValvulas: (especificacionDeValvulas aplicarEn: proyecto).	plan documentarGasAVender: (especificacionDeVentaDeGas aplicarEn: proyecto).	plan asignarVolumenes: (especificacionDeVolumenes aplicarEn: proyecto).	! !!PoliticaDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/30/2017 20:31'!especificarValvulas: unaEspecificacionDePolitica	especificacionDeValvulas := unaEspecificacionDePolitica! !!PoliticaDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/30/2017 20:31'!especificarVolumenes: unaEspecificacionDePoliticas	especificacionDeVolumenes := unaEspecificacionDePoliticas! !!PoliticaDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 05:13'!especificarVolumenenDeGasAVender: unaEspecificacionDePoliticas	especificacionDeVentaDeGas := unaEspecificacionDePoliticas! !!PoliticaDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/30/2017 20:32'!especificarMotivo: unaEspecificacionDePoliticas	especificacionDeMotivo := unaEspecificacionDePoliticas! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PoliticaDeDistribucion class	instanceVariableNames: ''!!PoliticaDeDistribucion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/9/2017 03:41'!conMotivoSegun: unaEspecificacionParaMotivo valvulasSegun: unaEspecificacionParaValvulas volumenesDeValvulasSegun: unaEspecificacionParaVolumenesDeValvulas volumenDeGasAVenderSegun: unaEspecificacionParaVolumenDeGas	^(self new) 	especificarMotivo: unaEspecificacionParaMotivo; 	especificarValvulas: unaEspecificacionParaValvulas; 	especificarVolumenes: unaEspecificacionParaVolumenesDeValvulas;	especificarVolumenenDeGasAVender: unaEspecificacionParaVolumenDeGas.! !PoliticaDeProyecto subclass: #PoliticaDeEquipamiento	instanceVariableNames: 'especificacionDeDevolucion especificacionDeAdquisicion'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/17/2017 03:46'!especificarAdquisiciones: unaEspecificacionDePoliticas	especificacionDeAdquisicion := unaEspecificacionDePoliticas! !!PoliticaDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/18/2017 02:25'!especificarPrescindibles: unaEspecificacionDePoliticas	especificacionDeDevolucion := unaEspecificacionDePoliticas! !!PoliticaDeEquipamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:09'!aplicarEn: unProyectoPetrolifero	| proyecto plan |	proyecto := unProyectoPetrolifero.	plan := proyecto planDeHoy planDeEquipamiento.	plan documentarAlquileresParaAdquirir: (especificacionDeAdquisicion aplicarEn: proyecto).	plan documentarAlquileresPrescindibles: (especificacionDeDevolucion aplicarEn: proyecto).	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PoliticaDeEquipamiento class	instanceVariableNames: ''!!PoliticaDeEquipamiento class methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/16/2017 06:01'!paraAdquirirSegun: unaEspecificacionDeAdquisicion yPrescindirSegun: unaEspecificacionDePrescindibles	^(self new) especificarAdquisiciones: unaEspecificacionDeAdquisicion; especificarPrescindibles: unaEspecificacionDePrescindibles ! !PoliticaDeEquipamiento subclass: #PoliticaDeEquipamientoOstentosa	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeEquipamientoOstentosa methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:19'!inicializar		especificacionDeAdquisicion := EspecificacionDePolitica especificar: [ :p |				"Alquila el alquiler mas caro que haya, la misma cantidad de veces que cantidad de parcelas haya"		| alquileresDeRIGSdeCatalogo cantidadAlquileresDelMasCaro documentosDeAlquilerDeRIGsYaTomados alquilerMasCaro cantidadDeParcelas cantidadDeValvulas |				documentosDeAlquilerDeRIGsYaTomados := p documentosDeAlquilerDeRIGsYaTomados.		alquileresDeRIGSdeCatalogo := ((p catalogoDeRecursosParaProyecto) subseccion: #('Equipamiento' 'RIGS')) publicaciones.		alquilerMasCaro := alquileresDeRIGSdeCatalogo detectMax: [ :alquiler | alquiler costoDiario ].		cantidadAlquileresDelMasCaro := documentosDeAlquilerDeRIGsYaTomados count: [ :documento | documento alquilerDocumentado = alquilerMasCaro ].		cantidadDeParcelas := p mapaDeParcelas parcelasEnMapa size.		cantidadDeValvulas := p valvulasDePozoYaInstaladas size.		OrderedCollection new: (((cantidadDeParcelas - cantidadDeValvulas) - cantidadAlquileresDelMasCaro) max: 0) withAll: alquilerMasCaro.		].	especificacionDeDevolucion := EspecificacionDePolitica especificar: [ :p | 		"No se molesta en cancelar y devolver ningun RIG alquilado"		OrderedCollection new.		].		descripcion := 'Alquilar desde el primer dia el alquiler mas caro que haya, la misma cantidad de veces que cantidad de parcelas haya'! !PoliticaDeProyecto subclass: #PoliticaDeExcavacion	instanceVariableNames: 'especificacionDeParcelas especificacionDeRIGS'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/31/2017 05:00'!especificarParcelas: unaEspecificacionDePoliticas	especificacionDeParcelas := unaEspecificacionDePoliticas! !!PoliticaDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/31/2017 05:00'!especificarRIGS: unaEspecificacionDePoliticas	especificacionDeRIGS := unaEspecificacionDePoliticas! !!PoliticaDeExcavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 05:09'!aplicarEn: unProyectoPetrolifero	| proyecto plan |	proyecto := unProyectoPetrolifero.	plan := proyecto planDeHoy planDeExcavacion.	plan documentarParcelas: (especificacionDeParcelas aplicarEn: proyecto).	plan asignarRIGS: (especificacionDeRIGS aplicarEn: proyecto).! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PoliticaDeExcavacion class	instanceVariableNames: ''!!PoliticaDeExcavacion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 17:02'!paraExcavarParcelasSegun: unaEspecificacionDeParcelas conRIGSsegun: unaEspecificacionDeRIGS	^(self new) especificarParcelas: unaEspecificacionDeParcelas; especificarRIGS: unaEspecificacionDeRIGS ! !PoliticaDeExcavacion subclass: #PoliticaDeExcavacionAcelerada	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeExcavacionAcelerada methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:30'!inicializar	especificacionDeParcelas := EspecificacionDePolitica especificar: [ :p |		"Excavo en todas las parcelas posibles, priorizando aquellas que tengan menos distancia al reservorio."		| parcelasAPerforar |		parcelasAPerforar := SortedCollection sortBlock: [ :x :y | (x distanciaAlReservorio) > (y distanciaAlReservorio) ].		parcelasAPerforar addAll: p mapaDeParcelas parcelasEnMapa.		parcelasAPerforar.		].		especificacionDeRIGS := EspecificacionDePolitica especificar: [ :p |		"Uso todos los RIGs disponibles, priorizando a los que tienen mayor poder de excavacion."		| rigsSeleccionados |		rigsSeleccionados := SortedCollection sortBlock: [ :x :y | (x poderDeExcavacion) < (y poderDeExcavacion) ].		rigsSeleccionados addAll: p rigsActualmenteEnDeposito.		rigsSeleccionados.		].		descripcion := 'Excavo en todas las parcelas que me sea posible priorizando aquellas que estes a menor distancia del reservorio, usando todos los rigs que tenga, y de estos priorizando aquellos con mayor poder de excavacion.'! !PoliticaDeExcavacion subclass: #PoliticaDeExcavacionPorPresion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeExcavacionPorPresion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:39'!inicializar	especificacionDeParcelas := EspecificacionDePolitica especificar: [ :p |		"Excavo en la mitad de las parcelas del mapa, priorizando aquellas que tengan mayor presion."		| parcelasAPerforar |		parcelasAPerforar := SortedCollection sortBlock: [ :x :y | (x presionInicial ) < (y presionInicial) ].		parcelasAPerforar addAll: p mapaDeParcelas parcelasEnMapa.		parcelasAPerforar first: ((parcelasAPerforar size) / 2) rounded.		parcelasAPerforar.		].		especificacionDeRIGS := EspecificacionDePolitica especificar: [ :p |		"Uso todos los RIGs disponibles, priorizando a los que tienen menor consumo diario."		| rigsSeleccionados |		rigsSeleccionados := SortedCollection sortBlock: [ :x :y | (x consumoDiario) > (y consumoDiario) ].		rigsSeleccionados addAll: p rigsActualmenteEnDeposito.		rigsSeleccionados.		].		descripcion := 'Excavo en la mitad de las parcelas priorizando aquellas que tengan mayor presion inicial, usando todos los rigs que tenga, y de estos priorizando aquellos con menor consumo diario.'! !PoliticaDeDistribucion subclass: #PoliticaDeExtraccionMaxima	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PoliticaDeExtraccionMaxima methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:27'!inicializar	especificacionDeMotivo := EspecificacionDePolitica especificar: [ :p |		"Si el dia anterior extraje menos de los que la red de distribucion soporta, extraigo, si no reinyecto lo maximo posible, si es necesario uso un camion cisterna."		| plan volumenesDeValvulas |		plan := (p archivoDePlaneamiento planDelDia: (p fechaDeHoy subtractDays: 1)) planDeDistribucion.		volumenesDeValvulas := plan valvulasAHabilitar sumNumbers: [ :val | plan volumenPara: val ].				((p capacidadDeRedDeDistribucion > 0 & (p capacidadActualDeAlmacenamientoDeAgua > 0)) | ((p capacidadActualDeAlmacenamientoDeAgua + (p volumenDeAguaAlmacenado)) isZero)) 		ifTrue: [ (Extraccion new) ]		ifFalse: [ ((p volumenDeAguaAlmacenado) isZero)			ifFalse: [ (ReinyeccionPorTanqueDeAgua new) ] 			ifTrue: [ (ReinyeccionPorCamionCisterna new) ] ]	].	especificacionDeValvulas := EspecificacionDePolitica especificar: [ :p |		"Uso todas las valvulas, priorizando las de mayor presion."		| valvulasEspecificadas |		valvulasEspecificadas := SortedCollection sortBlock: [ :x :y | (x medirPresionDeBocaDePozo) < (y medirPresionDeBocaDePozo) ].		valvulasEspecificadas addAll: (p valvulasDePozoYaInstaladas).		valvulasEspecificadas.		].		especificacionDeVolumenes := EspecificacionDePolitica especificar: [ :p |		"A cada valvula el asigna el resultado de dividir la capacidad de la red por la cantidad de valvulas"		| volumenesEspecificados |				volumenesEspecificados := OrderedCollection new: (p valvulasDePozoYaInstaladas size) withAll: ( p capacidadDeRedDeDistribucion) / ((p valvulasDePozoYaInstaladas size) max: 1).		volumenesEspecificados := OrderedCollection new: (p valvulasDePozoYaInstaladas size) withAll: 100.		volumenesEspecificados.				].		especificacionDeVentaDeGas := EspecificacionDePolitica especificar: [ :p |		"Siempre vendo todo lo que tenga almacenado de gas."		 p volumenDeGasAlmacenado 		].		descripcion := 'Extraer todo lo que la red de distribucion permita, repartiendolo entre cada valvula, siempre vendiendo lo que se haya extraido de gas y reinyectando solo si la suma de los volumenes configurados en las valvulas es mayor a la capacidad de la red' ! !Object subclass: #Producto	instanceVariableNames: 'porcentajeDePetroleo porcentajeDeAgua porcentajeDeGas'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Producto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/25/2017 22:51'!proporcionDePetroleo	^porcentajeDePetroleo! !!Producto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/25/2017 22:50'!proporcionDeAgua	^porcentajeDeAgua! !!Producto methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/25/2017 22:51'!proporcionDeGas	^porcentajeDeGas! !!Producto methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 03:00'!inicializarConProporcionesDeAgua: unPorcentajeDeAgua Gas: unPorcentajeDeGas yPetroleo: unPorcentajeDePetroleo	porcentajeDePetroleo := (unPorcentajeDePetroleo min: 1) max: 0.	porcentajeDeGas := (unPorcentajeDeGas min: (1 - porcentajeDePetroleo)) max: 0.	porcentajeDeAgua := (1 - porcentajeDeGas - porcentajeDePetroleo) max: 0.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Producto class	instanceVariableNames: ''!!Producto class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 22:07'!conProporcionesDeAgua: unaProporcionDeAgua Gas: unaProporcionDeGas yPetroleo: unaProporcionDePetroleo	^(self new) inicializarConProporcionesDeAgua: unaProporcionDeAgua Gas: unaProporcionDeGas yPetroleo: unaProporcionDePetroleo ! !Object subclass: #ProyectoPetrolifero	instanceVariableNames: 'archivoDePlaneamiento escenario'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!capacidadActualDeAlmacenamientoDeAgua	^escenario infraestructuraDisponible redDeDistribucionConectada capacidadActualDeAlmacenamientoDeAgua! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:37'!capacidadDeRedDeDistribucion	^escenario infraestructuraDisponible redDeDistribucionConectada capacidadParaProducto: escenario reservorioDelYacimiento tipoDeProductoAlbergado.	! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/30/2017 23:20'!fechaDeHoy	^escenario calendario fechaDeHoy! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/18/2017 00:36'!planDeHoy	^(archivoDePlaneamiento planDelDia: (self fechaDeHoy))! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/13/2017 04:52'!fechaDeInicioDelProyecto	^escenario calendario fechaDeInicio! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/13/2017 04:24'!registroDeActividadesDelProyecto	^escenario registroDeActividades! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 03:59'!alquileresDeRIGsActualmenteTomados	^escenario infraestructuraDisponible depositoDeEquipamiento documentosDeAlquileresRegistrados! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!volumenDeGasAlmacenado	^escenario infraestructuraDisponible redDeDistribucionConectada volumenDeGasAlmacenado ! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:09'!prepararNuevaDocumentacionDelDia	| plan valvulas parcelas |	plan := (PlanDiarioDeProyecto new) inicializar.	valvulas := OrderedCollection withAll: (escenario infraestructuraDisponible redDeDistribucionConectada valvulasDePozoConectadas).	plan planDeDistribucion inicializarConValvulas: valvulas.	parcelas := OrderedCollection withAll: (escenario mapaDeParcelas parcelasEnMapa).	plan planDeExcavacion inicializarConParcelas: parcelas.	archivoDePlaneamiento archivarPlan: plan deFecha: (self fechaDeHoy).		plan planDeEquipamiento documentarAlquileresParaAdquirir: (escenario catalogoDeRecursosParaProyecto seccion: 'Equipamiento') confeccionarCopia.	plan planDeEquipamiento documentarAlquileresPrescindibles: (escenario catalogoDeRecursosParaProyecto seccion: 'Equipamiento') confeccionarCopia.	plan planDeConstruccion documentarObrasDeConstruccion: (escenario catalogoDeRecursosParaProyecto seccion: 'ObrasDeConstruccion') confeccionarCopia.! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!capacidadActualDeAlmacenamientoDeGas	^escenario infraestructuraDisponible redDeDistribucionConectada capacidadActualDeAlmacenamientoDeGas! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!capacidadDeSeparacion	^escenario infraestructuraDisponible redDeDistribucionConectada capacidadDeSeparacion! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!volumenDeAguaAlmacenado	^escenario infraestructuraDisponible redDeDistribucionConectada volumenDeAguaAlmacenado ! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!valvulasDePozoYaInstaladas	^escenario infraestructuraDisponible redDeDistribucionConectada valvulasDePozoConectadas.	! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 02:44'!mapaDeParcelas	^escenario mapaDeParcelas.	! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 02:47'!rigsActualmenteEnDeposito	^escenario infraestructuraDisponible depositoDeEquipamiento inventarioDeRIGS! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 12:59'!archivoDePlaneamiento	^archivoDePlaneamiento		 ! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 23:35'!inicializarParaEscenario: unEscenarioDeSimulacion 	escenario := unEscenarioDeSimulacion.	archivoDePlaneamiento := ArchivoDePlaneamiento construirArchivo.	self prepararNuevaDocumentacionDelDia.	! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 17:34'!catalogoDeRecursosParaProyecto	| catalogo |		catalogo := (escenario catalogoDeRecursosParaProyecto) confeccionarCopia.	catalogo publicaciones do: [ :pub | pub asignarRegistroDeActividades: escenario registroDeActividades ].	^catalogo	! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:56'!obrasActualmenteEnConstruccion	^escenario infraestructuraDisponible documentosDeObrasEnConstruccion! !!ProyectoPetrolifero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 04:28'!documentosDeAlquilerDeRIGsYaTomados	^escenario infraestructuraDisponible depositoDeEquipamiento documentosDeAlquileresRegistrados! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ProyectoPetrolifero class	instanceVariableNames: ''!!ProyectoPetrolifero class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/13/2017 04:01'!basadoEn: unEscenarioDeSimulacion	^(self new) inicializarParaEscenario: unEscenarioDeSimulacion.! !Object subclass: #RIG	instanceVariableNames: 'poderDeExcavacion consumoDiario modelo alquiler'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/10/2017 22:28'!poderDeExcavacion	^ poderDeExcavacion ! !!RIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/18/2017 20:19'!modelo	^modelo! !!RIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 17:07'!inicializarConPotencia: unPoderDeExcavacion yConsumoDiario: unConsumo	poderDeExcavacion  := unPoderDeExcavacion.	consumoDiario := unConsumo.	modelo := unPoderDeExcavacion hash bitXor: unConsumo hash.! !!RIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/10/2017 22:05'!consumoDiario	^consumoDiario! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RIG class	instanceVariableNames: ''!!RIG class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 17:08'!diseñarConPotencia: unPoderDeExcavacion yConsumoDiario: unConsumo	^(self new) inicializarConPotencia: unPoderDeExcavacion yConsumoDiario: unConsumo! !Object subclass: #RedDeAlmacenamiento	instanceVariableNames: 'redDeAcueducto redDeGasoducto tanquesDeAgua tanquesDeGas'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/4/2017 18:53'!capacidadTotalParaGas	^tanquesDeGas ifNotEmptyDo: [ :tan1 | tan1 sumNumbers: [ :tan2 | tan2 capacidadTotal ]] ifEmpty: [ 0 ].! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/4/2017 18:53'!capacidadTotalParaAgua	^tanquesDeAgua ifNotEmptyDo: [ :tan1 | tan1 sumNumbers: [ :tan2 | tan2 capacidadTotal ]] ifEmpty: [ 0 ].! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 05:50'!inicializar	tanquesDeAgua := OrderedCollection new.	tanquesDeGas := OrderedCollection new.! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/4/2017 19:01'!incorporarTanqueDeGas: unTanqueDeGas	unTanqueDeGas nroDeTanque: (1 + (tanquesDeGas size)).	tanquesDeGas add: unTanqueDeGas! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:30'!extraerVolumenDeAguaAlmacenada: unVolumen	| volNoAceptado |	volNoAceptado := unVolumen max: 0.	1 to: tanquesDeAgua size do: [ :index | volNoAceptado := volNoAceptado - ((tanquesDeAgua at: index) extraer: volNoAceptado) ].	^volNoAceptado! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 19:41'!recibirAgua: unVolumen	| volNoAceptado |	volNoAceptado := unVolumen max: 0.	1 to: tanquesDeAgua size do: [ :index | volNoAceptado := volNoAceptado - ((tanquesDeAgua at: index) almacenar: volNoAceptado) ].	^volNoAceptado! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:03'!capacidadActualDeAlmacenamientoDeAgua	tanquesDeAgua ifNotEmptyDo: [ :tan1 | ^tan1 sumNumbers: [ :tan2 | tan2 capacidadActual ]] ifEmpty: [ ^0 ].! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/4/2017 18:55'!incorporarTanqueDeAgua: unTanqueDeAgua	unTanqueDeAgua nroDeTanque: (1 + (tanquesDeAgua size)).	tanquesDeAgua add: unTanqueDeAgua! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 19:41'!recibirGas: unVolumen	| volNoAceptado |	volNoAceptado := unVolumen max: 0.	1 to: tanquesDeGas size do: [ :index | volNoAceptado := volNoAceptado - ((tanquesDeGas at: index) almacenar: volNoAceptado) ].	^volNoAceptado! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:04'!capacidadActualDeAlmacenamientoDeGas	tanquesDeGas ifNotEmptyDo: [ :tan1 | ^tan1 sumNumbers: [ :tan2 | tan2 capacidadActual ]] ifEmpty: [ ^0 ].! !!RedDeAlmacenamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:31'!extraerVolumenDeGasAlmacenado: unVolumen	| volNoAceptado |	volNoAceptado := unVolumen max: 0.	1 to: tanquesDeGas size do: [ :index | volNoAceptado := volNoAceptado - ((tanquesDeGas at: index) extraer: volNoAceptado) ].	^volNoAceptado! !Object subclass: #RedDeDistribucion	instanceVariableNames: 'redDePozos redDeSeparacion redDeAlmacenamiento petroleoProcesado modalidadDeDistribucion'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:02'!inicializar	redDePozos := (RedDePozos new) inicializar.	redDeSeparacion := (RedDeSeparacion new) inicializar.	redDeAlmacenamiento := (RedDeAlmacenamiento new) inicializar.	redDePozos conectarARedDeDistribucion: self.	petroleoProcesado := 0.! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:03'!capacidadActualDeAlmacenamientoDeAgua	^redDeAlmacenamiento capacidadActualDeAlmacenamientoDeAgua! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 02:33'!incorporarTanqueDeGas: unTanqueDeGas	redDeAlmacenamiento incorporarTanqueDeGas: unTanqueDeGas! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 05:32'!capacidadParaProducto: unProducto	| capacidadDeAlmacenajeDeAgua capacidadDeAlmacenajeDeGas capacidadDeSeparacion proporcionDeAgua proporcionDeGas volTotalSegunAgua volTotalSegunGas |	capacidadDeAlmacenajeDeAgua := redDeAlmacenamiento capacidadActualDeAlmacenamientoDeAgua.	capacidadDeAlmacenajeDeGas := redDeAlmacenamiento capacidadActualDeAlmacenamientoDeGas.	capacidadDeSeparacion := redDeSeparacion capacidadDeSeparacion.	proporcionDeAgua := unProducto proporcionDeAgua.	proporcionDeGas := unProducto proporcionDeGas.	(proporcionDeAgua isZero & proporcionDeGas isZero) ifTrue: [		^capacidadDeSeparacion 	].	(proporcionDeGas isZero) ifTrue: [		volTotalSegunAgua := (((1 - proporcionDeAgua)/proporcionDeAgua) + 1)*capacidadDeAlmacenajeDeAgua.		^(volTotalSegunAgua min: capacidadDeSeparacion).	].	(proporcionDeAgua isZero) ifTrue: [		volTotalSegunGas := (((1 - proporcionDeGas)/proporcionDeGas) + 1)*capacidadDeAlmacenajeDeGas.		^volTotalSegunGas min: capacidadDeSeparacion.	].	volTotalSegunAgua := (((1 - proporcionDeAgua)/proporcionDeAgua) + 1)*capacidadDeAlmacenajeDeAgua.	volTotalSegunGas := (((1 - proporcionDeGas)/proporcionDeGas) + 1)*capacidadDeAlmacenajeDeGas.	^((volTotalSegunAgua min: volTotalSegunGas) min: capacidadDeSeparacion).! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:30'!extraerVolumenDeAguaAlmacenada: unVolumen	^redDeAlmacenamiento extraerVolumenDeAguaAlmacenada: (unVolumen max: 0)! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 02:16'!incorporarTanqueDeAgua: unTanqueDeAgua	redDeAlmacenamiento incorporarTanqueDeAgua: unTanqueDeAgua! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:32'!valvulasDePozoConectadas	^redDePozos valvulasDePozoConectadas! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:04'!volumenDeGasAlmacenado	^((redDeAlmacenamiento capacidadTotalParaGas) - ( redDeAlmacenamiento capacidadActualDeAlmacenamientoDeGas))! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:02'!habilitarValvulasDePozo: unaColeccionDeValvulasDePozo	| volResultante volDeAgua volDeGas |		petroleoProcesado := 0.	volResultante := redDePozos habilitarValvulasDePozo: unaColeccionDeValvulasDePozo.	volDeAgua := redDeSeparacion separarAgua.	redDeAlmacenamiento recibirAgua:  volDeAgua.	volDeGas := redDeSeparacion separarGas.	redDeAlmacenamiento recibirGas: volDeGas.	petroleoProcesado := redDeSeparacion separarPetroleo.	^volResultante! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:04'!capacidadActualDeAlmacenamientoDeGas	^redDeAlmacenamiento capacidadActualDeAlmacenamientoDeGas! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 05:26'!capacidadDeSeparacion	^(redDeSeparacion capacidadDeSeparacion)! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:35'!instalarValvulaDePozoEn: unaParcela conectadaA: unReservorio	redDePozos instalarValvulaDePozoEn: unaParcela conectadaA: unReservorio! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 00:03'!volumenDeAguaAlmacenado	^((redDeAlmacenamiento capacidadTotalParaAgua) - ( redDeAlmacenamiento capacidadActualDeAlmacenamientoDeAgua))! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 06:26'!procesarCaudalDe: unVolumen deProducto: unProducto	| volAceptado |	volAceptado := ((self capacidadParaProducto: unProducto) min: unVolumen) max: 0.	volAceptado := redDeSeparacion		recibirCaudalDe: volAceptado		deProducto: unProducto.	^volAceptado! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:31'!extraerVolumenDeGasAlmacenado: unVolumen	^redDeAlmacenamiento extraerVolumenDeGasAlmacenado: (unVolumen max: 0)! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:34'!volumenDePetroleoProcesadoDelUltimoCaudal	^petroleoProcesado! !!RedDeDistribucion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 02:41'!incorporarPlantaSeparadora: unaPlantaDeSeparacion	redDeSeparacion incorporarPlantaSeparadora: unaPlantaDeSeparacion! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RedDeDistribucion class	instanceVariableNames: ''!!RedDeDistribucion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/29/2017 02:35'!construir	^(self new) inicializar.! !Object subclass: #RedDePozos	instanceVariableNames: 'valvulas redDeDistribucion reservorioAlQueDesembocanLosPozos'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!redDeDistribucionConectada	^redDeDistribucion! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 06:20'!habilitarValvulasDePozo: unaColeccionDeValvulas		| volumenResultante |	volumenResultante := 0.	unaColeccionDeValvulas do: [ :val | 		volumenResultante := volumenResultante + (val habilitar) ].	^volumenResultante		! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 04:32'!valvulasDePozoConectadas	^valvulas! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/28/2017 17:44'!cantidadDePozos	^valvulas size.! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:32'!instalarValvulaDePozoEn: unaParcela conectadaA: unReservorio	| nuevaValvula |	nuevaValvula := ValvulaDePozo acoplarA: self en: unaParcela.	reservorioAlQueDesembocanLosPozos := unReservorio.	valvulas add: nuevaValvula.! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:32'!inicializar	valvulas := OrderedCollection new! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 20:22'!reservorioAlQueDesembocanLosPozos	^reservorioAlQueDesembocanLosPozos! !!RedDePozos methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:07'!conectarARedDeDistribucion: unaRedDeDistribucion	redDeDistribucion := unaRedDeDistribucion.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RedDePozos class	instanceVariableNames: ''!!RedDePozos class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 01:55'!construir	^(self new) inicializar.! !Object subclass: #RedDeSeparacion	instanceVariableNames: 'plantasSeparadoras'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 16:58'!inicializar	plantasSeparadoras := (OrderedCollection new).! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/23/2017 02:54'!incorporarPlantaSeparadora: unaPlantaDeSeparacion	unaPlantaDeSeparacion nroDePlanta: (1 + (plantasSeparadoras size)).	plantasSeparadoras add: unaPlantaDeSeparacion! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 01:54'!separarPetroleo	| volPetroleo |	volPetroleo := plantasSeparadoras ifNotEmptyDo: [ :plantas | plantas sumNumbers: [ :planta | planta separarPetroleo ] ] ifEmpty: [ 0 ].	^volPetroleo.! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 18:36'!separarGas	| volGas |	volGas := plantasSeparadoras ifNotEmptyDo: [ :plantas | plantas sumNumbers: [ :planta | planta separarGas ] ] ifEmpty: [ 0 ].	^volGas.! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 18:27'!separarAgua	| volAgua |	volAgua := plantasSeparadoras ifNotEmptyDo: [ :plantas | plantas sumNumbers: [ :planta | planta separarAgua ] ] ifEmpty: [ 0 ].	^volAgua.! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:01'!capacidadDeSeparacion	plantasSeparadoras ifNotEmptyDo: [ :plantas | ^plantas sumNumbers: [ :planta | planta capacidadActual ]] ifEmpty: [ ^0 ].! !!RedDeSeparacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 23:01'!recibirCaudalDe: unVolumen deProducto: unProducto	| volNoAceptadoTotal volAceptadoTotal |	volNoAceptadoTotal := unVolumen max: 0.	volAceptadoTotal := 0.	plantasSeparadoras do: [ :planta |		| volAceptadoLocal |		volAceptadoLocal := planta recibirCaudalDe: volNoAceptadoTotal deProducto: unProducto.		volNoAceptadoTotal := volNoAceptadoTotal - volAceptadoLocal.		volAceptadoTotal := volAceptadoTotal + volAceptadoLocal ].	^volAceptadoTotal.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!RedDeSeparacion class	instanceVariableNames: ''!!RedDeSeparacion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 01:54'!construir	^(self new) inicializar.! !Object subclass: #Registrador	instanceVariableNames: 'registro'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Registrador methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 03:47'!registrar: unEvento	registro registrar: unEvento.! !!Registrador methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/18/2017 20:33'!asignarRegistroDeActividades: unRegistro	registro := unRegistro.! !Registrador subclass: #AlquilerDeRIG	instanceVariableNames: 'rig diasMinimos costoDiario'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 04:06'!costoDiario	^costoDiario! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 20:35'!rigEnAlquiler	^rig! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 20:39'!minimaCantidadDeDiasDeAlquiler: unaCantidad	diasMinimos := unaCantidad! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 18:35'!retirarDe: unEscenarioDeSimulacion con: unFletador	| datos |		unFletador devolverRIGdeAlquiler: self en: unEscenarioDeSimulacion.	datos := OrderedCollection new.	datos add: rig modelo; add: costoDiario; add: diasMinimos.	self registrar: ('Se dejo de alquilar un rig modelo {1} a costo diario de {2} por un minimo de {3} dias' format: datos)! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 00:49'!transportarA: unEscenarioDeSimulacion con: unFletador		| datos |	(unFletador depositarRIGdeAlquiler: self en: unEscenarioDeSimulacion)	ifTrue: [		datos := OrderedCollection new.		datos add: rig modelo; add: costoDiario; add: diasMinimos.	self registrar: ('Se alquilo un rig modelo {1} a costo diario de {2} pesos por un minimo de {3} dias' format: datos)].! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 01:07'!describirAlquiler	| datos |	datos := OrderedCollection new.	datos add: rig modelo.	^('RIG modelo {1}' format: datos).! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 20:37'!inicializarConRIG: unRIG	rig := unRIG.	diasMinimos := 0! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 01:06'!diasMinimos	^diasMinimos! !!AlquilerDeRIG methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 04:05'!fijarCostoDiario: unCosto	costoDiario := unCosto! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AlquilerDeRIG class	instanceVariableNames: ''!!AlquilerDeRIG class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 20:37'!deRIG: unRIG aCosto: unCosto	^(self new) inicializarConRIG: unRIG; fijarCostoDiario: unCosto ! !Registrador subclass: #Excavacion	instanceVariableNames: 'parcela distanciaExcavada rig'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Excavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:21'!parcelaEnExcavacion	^parcela! !!Excavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:22'!rigInstalado	^rig! !!Excavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/31/2017 05:57'!distanciaExcavada	^distanciaExcavada! !!Excavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 14:50'!inicializarEnParcela: unaParcela excavadaPor: unRIG	parcela := unaParcela.	rig := unRIG.	distanciaExcavada := 0.	self asignarRegistroDeActividades: RegistroDeActividades new.! !!Excavacion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/16/2017 05:42'!proseguirConExcavacion	| excavado datos ubicacion latitud longitud| 	excavado := parcela excavarCon: rig habiendoExcavado: distanciaExcavada.	distanciaExcavada := distanciaExcavada + excavado.		ubicacion := parcela ubicacion.	latitud := ubicacion x.	longitud := ubicacion y.	datos := OrderedCollection new.	datos add: excavado; add: latitud; add: longitud.	(parcela distanciaAlReservorio = distanciaExcavada) 		ifTrue: [self registrar: ('Se termino de excavar en la parcela de ubicacion ({2},{3}) tras excavar {1}m.' format: datos)] 		ifFalse: [self registrar: ('Se excavaron {1}m en la parcela de ubicacion ({2},{3}).' format: datos)].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Excavacion class	instanceVariableNames: ''!!Excavacion class methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/31/2017 05:43'!en: unaParcela con: unRIG	^(self new) inicializarEnParcela: unaParcela excavadaPor: unRIG! !Registrador subclass: #ObraDeConstruccion	instanceVariableNames: 'presupuesto modelo diasNecesarios'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ObraDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 01:08'!describirObra	^self subclassResponsibility! !!ObraDeConstruccion methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 21