Object subclass: #AdministradorFinanciero	instanceVariableNames: 'cotizacionDelGas cotizacionDelPetroleo'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!venderGasEn: unEscenarioDeSimulacion	| registroDeContaduria redDeDistribucion volumenAVender plan datos |		plan := unEscenarioDeSimulacion proyectoEnCurso planDeHoy planDeDistribucion.	registroDeContaduria := unEscenarioDeSimulacion registroDeContaduria.	redDeDistribucion := unEscenarioDeSimulacion infraestructuraDisponible redDeDistribucionConectada.	volumenAVender := redDeDistribucion extraerVolumenDeGasAlmacenado: plan volumenDeGasAVender.	datos := OrderedCollection new.	datos add: volumenAVender*cotizacionDelGas.	registroDeContaduria registrarGanancia: volumenAVender*cotizacionDelGas debidoA: ('Se vendieron {1}m3 de gas' format: datos).		! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 06:22'!venderPetroleoEn: unEscenarioDeSimulacion	| registroDeContaduria redDeDistribucion volumenAVender datos |		registroDeContaduria := unEscenarioDeSimulacion registroDeContaduria.	redDeDistribucion := unEscenarioDeSimulacion infraestructuraDisponible redDeDistribucionConectada.	volumenAVender := redDeDistribucion volumenDePetroleoProcesadoDelUltimoCaudal.	datos := OrderedCollection new.	datos add: volumenAVender*cotizacionDelPetroleo.	registroDeContaduria registrarGanancia: volumenAVender*cotizacionDelPetroleo debidoA: ('Se vendieron {1}m3 de petroleo' format: datos).		! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 23:49'!cotizarGasA: unMontor	cotizacionDelGas := unMontor! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 00:32'!registrarGastosDeAlquileresEn: unEscenarioDeSimulacion	| registroDeContaduria alquileres datos |		registroDeContaduria := unEscenarioDeSimulacion registroDeContaduria.	alquileres := unEscenarioDeSimulacion infraestructuraDisponible depositoDeEquipamiento documentosDeAlquileresRegistrados collect: [:documento | documento alquilerDocumentado ].	alquileres do: [ :alquiler |		datos := OrderedCollection new.		datos add: alquiler costoDiario.		registroDeContaduria registrarGasto: (alquiler costoDiario) debidoA: (('Se pago {1} pesos del alquiler de un ', (alquiler describirAlquiler)) format: datos).	]		! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 00:32'!registrarGastosEnObrasDeConstruccionEn: unEscenarioDeSimulacion	| registroDeContaduria obrasAConstruir plan datos |		plan := unEscenarioDeSimulacion proyectoEnCurso planDeHoy planDeConstruccion.	registroDeContaduria := unEscenarioDeSimulacion registroDeContaduria.	obrasAConstruir := plan obrasAConstruir.	obrasAConstruir do: [ :obra |		datos := OrderedCollection new.		datos add: obra presupuestoNecesario.		registroDeContaduria registrarGasto: (obra presupuestoNecesario) debidoA: (('Se presupuesto  {1} pesos para la construccion de ', (obra describirObra)) format: datos).	]		! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 00:32'!realizarContaduriaEn: unEscenarioDeSimulacion	self venderPetroleoEn: unEscenarioDeSimulacion.	self venderGasEn: unEscenarioDeSimulacion.	self registrarGastosDeAlquileresEn: unEscenarioDeSimulacion.	self registrarGastosEnObrasDeConstruccionEn: unEscenarioDeSimulacion.! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 01:18'!inicializar	cotizacionDelGas := 1.	cotizacionDelPetroleo := 1.! !!AdministradorFinanciero methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 23:48'!cotizarPetroloeA: unMontor	cotizacionDelPetroleo := unMontor! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AdministradorFinanciero class	instanceVariableNames: ''!!AdministradorFinanciero class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 00:09'!nuevoContadorFinanciero	^(self new) inicializar! !Object subclass: #ArchivoDePlaneamiento	instanceVariableNames: 'planesDeDia'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!ArchivoDePlaneamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 13:02'!planDelDia: unaFecha	^planesDeDia at: unaFecha ifAbsent: [ planesDeDia at: (planesDeDia keys last) ].! !!ArchivoDePlaneamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 00:59'!inicializar	planesDeDia := Dictionary new.! !!ArchivoDePlaneamiento methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/30/2017 23:49'!archivarPlan: unPlanDeDia deFecha: unaFecha	planesDeDia at: unaFecha put: unPlanDeDia 	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ArchivoDePlaneamiento class	instanceVariableNames: ''!!ArchivoDePlaneamiento class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 23:34'!construirArchivo	^(self new) inicializar! !Object subclass: #Calendario	instanceVariableNames: 'fechaDeHoy fechaDeInicio'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Calendario methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 12:57'!fechaDeInicio	^fechaDeInicio! !!Calendario methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/12/2017 16:23'!fechaDeHoy	^fechaDeHoy! !!Calendario methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/6/2017 04:39'!inicializarEnFecha: unaFecha	fechaDeInicio := unaFecha.	fechaDeHoy := unaFecha.! !!Calendario methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/14/2017 03:32'!avanzarUnDia	fechaDeHoy := fechaDeHoy next.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Calendario class	instanceVariableNames: ''!!Calendario class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 19:00'!iniciadoEn: unaFecha	^(self new) inicia