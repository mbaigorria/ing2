s |	datos := OrderedCollection new.	datos add: tanque modelo.	^('tanque de gas modelo {1}' format: datos).! !!ObraDeConstruccionDeTanqueDeGas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 15:23'!continuarConstruccionCon: unaConstructora en: unEscenarioDeSimulacion trasConstruirDurante: unaCantidadDeDias	| datos diasRestantes diasDeTrabajo |		diasDeTrabajo := unaCantidadDeDias + 1.	diasRestantes := (diasNecesarios - diasDeTrabajo) max: 0.	(diasRestantes isZero) ifTrue:[ 		unaConstructora inaugurarTanqueDeGas: tanque en: unEscenarioDeSimulacion.		datos := OrderedCollection new.		datos add: tanque modelo.		self registrar: ('Se termino de construir un tanque de gas modelo {1}.' format: datos).		^self.		].		datos := OrderedCollection new.	datos add: tanque modelo; add: diasRestantes.	(diasDeTrabajo = 1) 	ifTrue: [ 		self registrar: ('Se empezo con la construccion de un tanque de gas modelo {1}, aun le faltan {2} dias para terminar.' format: datos).		 ]	 ifFalse: [	self registrar: ('Se continuo con la construccion de un tanque de gas modelo {1}, aun le faltan {2} dias para terminar.' format: datos).	]! !!ObraDeConstruccionDeTanqueDeGas methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/11/2017 15:59'!tanqueDeGasAConstruir	^tanque! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ObraDeConstruccionDeTanqueDeGas class	instanceVariableNames: ''!!ObraDeConstruccionDeTanqueDeGas class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 22:23'!de: unTanqueDeGas enDias: unaCantidadDeDias conPresupuestoDe: unPresupuesto	^(self new) inicializarConTanque: unTanqueDeGas; diasNecesarios: unaCantidadDeDias; presupuestoNecesario: unPresupuesto. ! !Registrador subclass: #PlantaSeparadora	instanceVariableNames: 'volumenDeGas volumenDeAgua volumenDePetroleo capacidadTotal nroDePlanta modelo'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/21/2017 15:13'!capacidadTotal	^capacidadTotal! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 01:52'!separarGas	| volGas datos |	volGas := volumenDeGas.	volumenDeGas := 0.	(volGas isZero) ifFalse: [		datos := OrderedCollection new.		datos add: volGas; add: nroDePlanta.	self registrar: ('Se separaron {1}m3 de gas en la planta numero {2}' format: datos)].	^volGas.	! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 05:51'!separarAgua	| volAgua datos |	volAgua := volumenDeAgua.	volumenDeAgua := 0.	(volAgua isZero) ifFalse: [		datos := OrderedCollection new.		datos add: volAgua; add: nroDePlanta.	self registrar: ('Se separaron {1}m3 de agua en la planta numero {2}' format: datos)].	^volAgua.	! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 01:53'!separarPetroleo	| volPetroleo datos |	volPetroleo := volumenDePetroleo.	volumenDePetroleo := 0.	(volPetroleo isZero) ifFalse: [		datos := OrderedCollection new.		datos add: volPetroleo; add: nroDePlanta.	self registrar: ('Se separaron {1}m3 de petroleo en la planta numero {2}' format: datos)].	^volPetroleo.	! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/12/2017 02:20'!capacidadActual	| volumenEnProcesamiento |	volumenEnProcesamiento := volumenDeAgua + volumenDeGas + volumenDePetroleo.	^(capacidadTotal - volumenEnProcesamiento).! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/23/2017 02:56'!nroDePlanta: unNumero	nroDePlanta:= unNumero! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 02:00'!inicializarConCapacidad: unaCapacidad	capacidadTotal := unaCapacidad max: 0.0.	volumenDeAgua := 0.0.	volumenDeGas := 0.0.	volumenDePetroleo := 0.0.	registro := (RegistroDeActividades new) inicializar.	modelo := unaCapacidad hash! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/20/2017 05:52'!recibirCaudalDe: unVolumen deProducto: unProducto	| volAceptado volumenEnProcesamiento |	volumenEnProcesamiento := volumenDeAgua + volumenDeGas + volumenDePetroleo.	volAceptado := ((capacidadTotal - volumenEnProcesamiento) min: unVolumen) max: 0.	volumenDeAgua := volumenDeAgua + (volAceptado*(unProducto proporcionDeAgua) max: 0).	volumenDeGas := volumenDeGas + (volAceptado*(unProducto proporcionDeGas) max: 0).	volumenDePetroleo := volumenDePetroleo + (volAceptado*(unProducto proporcionDePetroleo) max: 0).	^volAceptado.! !!PlantaSeparadora methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/19/2017 04:04'!modelo	^capacidadTotal hash! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!PlantaSeparadora class	instanceVariableNames: ''!!PlantaSeparadora class methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/25/2017 21:43'!construirConCapacidad: unaCapacidad	^(self new) inicializarConCapacidad: unaCapacidad! !Object subclass: #RegistroDeActividades	instanceVariableNames: 'actividadesDeDia calendario'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!RegistroDeActividades methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 19:14'!sincronizarConCalendario: unCalendario	calendario := unCalendario! !!RegistroDeActividades methodsFor: 'as yet unclassified' stamp: 'Cherubini 6/20/2017 14:00'!diasConActividad	^actividadesDeDia keys! !!RegistroDeActividades methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 19:13'!inicializar	actividadesDeDia := OrderedDictionary new.	calendario := Calendario iniciadoEn: (Date today).! !!RegistroDeActividades methodsFor: 'as yet unclassified' stamp: 'Cherubini 5/26/2017 03:46'!registrar: unEvento	| hoy actividades |	hoy := calendario fechaDeHoy.	actividades := actividadesDeDia at: hoy ifAbsent: [ actividadesDeDia at: hoy put: (OrderedCollection new) ].	actividades add: unEvento! !!RegistroDeActivid