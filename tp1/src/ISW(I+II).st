narioDeSimulacion registroDeActividades.			unEscenarioDeSimulacion mapaDeParcelas indicarNuevaExcavacion: excavacion ]! !!Excavador methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:27'!realizarExcavacionesDe: unEscenarioDeSimulacion	| escenario plan deposito parcelas mapa terminadas redDeDistribucion |	escenario := unEscenarioDeSimulacion.	redDeDistribucion := escenario infraestructuraDisponible redDeDistribucionConectada.	mapa := escenario mapaDeParcelas.	deposito := escenario infraestructuraDisponible depositoDeEquipamiento.	plan := escenario proyectoEnCurso planDeHoy planDeExcavacion.	parcelas := plan parcelasAPerforar.	parcelas do: [ :par | 		| rig |		rig := plan rigAsignadoA: par. 		rig ifNotNil: [ 		par perforarCon: rig en: unEscenarioDeSimulacion mediante: self ]].	terminadas := mapa proseguirConExcavaciones.	terminadas do: [ :exc | 		redDeDistribucion instalarValvulaDePozoEn: (exc parcelaEnExcavacion) conectadaA: (escenario reservorioDelYacimiento).		escenario infraestructuraDisponible depositoDeEquipamiento depositarRIG: (exc rigInstalado).		].	redDeDistribucion valvulasDePozoConectadas do: [ :valvula | valvula asignarRegistroDeActividades: unEscenarioDeSimulacion registroDeActividades ].	! !!Excavador methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/6/2017 01:07'!configurarCupoMaximoDePozos: unaCantidad	cupoMaximoDePozos := unaCantidad! !!Excavador methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/5/2017 23:54'!inicializar	cupoMaximoDePozos := 10.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Excavador class	instanceVariableNames: ''!!Excavador class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 00:04'!nuevoExcavador	^(self new) inicializar! !Object subclass: #Extractor	instanceVariableNames: 'distribuidor'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Extractor methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/19/2017 19:31'!realizarExtracionEn: unEscenarioDeSimulacion	| redDeDistribucion reservorio producto valvulas cantDeHabilitadas plan |	redDeDistribucion := unEscenarioDeSimulacion infraestructuraDisponible redDeDistribucionConectada.	reservorio := unEscenarioDeSimulacion reservorioDelYacimiento.	producto := reservorio tipoDeProductoAlbergado.	plan := unEscenarioDeSimulacion proyectoEnCurso planDeHoy planDeDistribucion.	valvulas := plan valvulasAHabilitar.	cantDeHabilitadas := valvulas size.	valvulas do: [ :val | 		| volPotencial volAceptable |		volPotencial := distribuidor calcularVolumenPotencialTeniendoPresionDe: (val medirPresionDeBocaDePozo) yCantidadDeValvulasHabilitadas: cantDeHabilitadas.		volAceptable := (volPotencial min: (plan volumenPara: val)) asFloat.		val configurarModalidad: (ModoDeExtraccion deVolumen: volAceptable).		].	redDeDistribucion habilitarValvulasDePozo: valvulas.	! !!Extractor methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/15/2017 16:50'!inicializarConDistribuidor: unDistribuidorDeProducto	distribuidor := unDistribuidorDeProducto.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Extractor class	instanceVariableNames: ''!!Extractor class methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 00:53'!nuevoExtractorPara: unDistribuidorDeProducto	^(self new) inicializarConDistribuidor: unDistribuidorDeProducto! !Object subclass: #Fletador	instanceVariableNames: 'transportadorDeRIGS'	classVariableNames: ''	poolDictionaries: ''	category: 'ISW(I+II)'!!Fletador methodsFor: 'as yet unclassified' stamp: 'Cherubini 7/18/2017 18:48'!realizarTransportesEn: unEscenarioDeSimulacion	| publicacionesParaAdquirir publicacionesParaPrescindir deposito |	publicacionesParaAdquirir := unEscenarioDeSimulacion proyectoEnCurso planDeHoy planDeEquipamiento alquileresDeEquiposParaAdquirir.	publicacionesParaAdquirir do: [ :pub | pub transportarA: unEscenarioDeSimulacion con: self ].	deposito := unEscenarioDeSimulacion i